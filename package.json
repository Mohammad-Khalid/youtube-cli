{
  "name": "youtube-cli",
  "description": "Manage youtube data through CLI",
  "version": "0.0.1-beta.1",
  "author": " @Mohammad-Khalid",
  "bin": {
    "youtube": "./bin/run"
  },
  "bugs": "https://github.com/Mohammad-Khalid/youtube-cli/issues",
  "dependencies": {
