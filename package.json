    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "airtable": "^0.8.1",
    "fs-extra": "^9.0.0",
    "googleapis": "^49.0.0",
    "inquirer": "^7.1.0",
    "ora": "^4.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
